{
  "$schema": "https://modelcontextprotocol.io/schemas/draft/2025-07-09/server.json",
  "description": "A sample MCP server with weather and random number tools",
  "name": "io.github.scopaz/SampleMcpServer", 
  "packages": [
    {
      "registry_name": "nuget",
      "name": "Scopaz.SampleMcpServer",
      "version": "1.0.0",
      "package_arguments": [],
      "environment_variables": [
        {
          "name": "WEATHER_CHOICES",
          "value": "{weather_choices}",
          "variables": {
            "weather_choices": {
              "description": "Comma separated list of weather descriptions to randomly select.",
              "is_required": true,
              "is_secret": false
            }
          }
        }
      ]
    }
  ],
  "repository": {
    "url": "https://github.com/scopaz/SampleMcpServer",
    "source": "github"
  },
  "version_detail": {
    "version": "1.0.0"
  }
}
